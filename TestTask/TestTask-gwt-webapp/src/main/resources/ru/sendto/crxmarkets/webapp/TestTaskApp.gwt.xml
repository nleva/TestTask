<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="TestTaskApp">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="ru.sendto.gwt.Html" />
	<inherits name="ru.sendto.gwt.client.Notifications" />
	<inherits name="ru.sendto.rest.UniversalRest" />
	<inherits name="ru.sendto.gwt.client.Shortcut" />
	<inherits name="ru.sendto.gwt.client.Bus" />
	<inherits name="ru.sendto.DtoCommon" />
	<inherits name="ru.sendto.crxmarkets.Dto" />
	<entry-point
		class="ru.sendto.crxmarkets.webapp.client.TestTaskApp" />
	<source path="client" />

	<extend-property name="user.agent" values="ie8" />
	<extend-property name="user.agent" values="gecko1_8" />
	<extend-property name="user.agent" values="safari" />
	<extend-property name="user.agent" values="ie9"
		fallback-value="ie8" />
	<extend-property name="user.agent" values="ie10" />
	
	<set-property name="webApiUsage" value="modern"/>
	
</module>